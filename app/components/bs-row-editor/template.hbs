{{#each model.cards as |card|}}
  {{bs-card-editor model=card
                  layoutEditor=layoutEditor
                  onCardDrag=(action 'onCardDrag')
                  onUpdateCardResizer=(action 'onUpdateCardResizer' )
                  onCardRemove=(action 'onCardRemove')}}
{{/each}}
{{card-resizer model=resizerModel
                 onShift=(action 'onShiftSharedEdge')}}
